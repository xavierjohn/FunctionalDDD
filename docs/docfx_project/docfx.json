{
  "metadata": [
    {
      "src": [
        {
          "src": "../..",
          "files": [
            "RailwayOrientedProgramming/src/RailwayOrientedProgramming.csproj",
            "DomainDrivenDesign/src/DomainDrivenDesign.csproj",
            "FluentValidation/src/FluentValidation.csproj",
            "Asp/src/Asp.csproj",
            "Http/src/Http.csproj",
            "PrimitiveValueObjects/src/PrimitiveValueObjects.csproj"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "globalMetadata": {
      "_appLogoPath": "/images/fddd.png"
